public class Quadrat {
    public static void main(String[] args) {
        int (numero = Integer.parseInt(args[0]);
        for (int li = 0; li < numero; li++) {
            dibuixaLinia();
        }

    public static dibuixaLinia() {
        for (int co = 0; co < numero; co++) {
            System.out.print(" X");
        }
    }

